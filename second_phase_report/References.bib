@article{Nossent2011,
abstract = {Complex environmental models are controlled by a high number of parameters. Accurately estimating the values of all these parameters is almost impossible. Sensitivity analysis (SA) results enable the selection of the parameters to include in a calibration procedure, but can also assist in the identification of the model processes. Additionally, a sensitivity analysis can yield crucial information on the use and meaning of the model parameters.$\backslash$n$\backslash$nThis paper presents a Sobol' sensitivity analysis for flow simulations by a SWAT model of the river Kleine Nete, with the objective to assess the first order, second order and total sensitivity effects. Confidence intervals for the resulting sensitivity indices are inferred by applying bootstrapping. The results indicate that the curve number value (CN2) is the most important parameter of the model and that no more than 9 parameters (out of 26) are needed to have an adequate representation of the model variability. The convergence of the parameter ranking for total sensitivity effects is relatively fast, which is promising for factor fixing purposes. It is also shown that the Sobol' sensitivity analysis enhances the understanding of the model, by e.g. pointing out 3 significant pairwise interactions.$\backslash$n$\backslash$nIn general, it can be concluded that the Sobol' sensitivity analysis can be successfully applied for factor fixing and factor prioritization with respect to the input parameters of a SWAT model, even with a limited number of model evaluations. The analysis also supports the identification of model processes, parameter values and parameter interaction effects.},
author = {Nossent, Jiri and Elsen, Pieter and Bauwens, Willy},
doi = {10.1016/j.envsoft.2011.08.010},
file = {:C$\backslash$:/Users/irfanlab/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nossent, Elsen, Bauwens - 2011 - Sobol' sensitivity analysis of a complex environmental model.pdf:pdf},
isbn = {1364-8152},
issn = {1364-8152},
journal = {Environmental Modelling {\&} Software},
keywords = {process identi fi cation,sensitivity analysis},
number = {12},
pages = {1515--1525},
title = {{Sobol' sensitivity analysis of a complex environmental model}},
url = {http://www.sciencedirect.com/science/article/pii/S1364815211001939{\%}5Cnhttp://www.sciencedirect.com/science/article/pii/S1364815211001939/pdfft?md5=a3e52e678ed8b2c2ca9cb2c613f039ea{\&}pid=1-s2.0-S1364815211001939-main.pdf},
