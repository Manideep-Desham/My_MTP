doi = {10.1016/j.paerosci.2010.10.001},
file = {:C$\backslash$:/Users/irfanlab/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Li, Nahon, Sharf - 2011 - Airship dynamics modeling A literature review.pdf:pdf},
