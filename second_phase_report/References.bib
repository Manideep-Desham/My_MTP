@article{Varmaghani2012,
abstract = {Accurate calculation of precipitable water vapor (PWV) in the atmosphere$\backslash$nhas always been a matter of importance for meteorologists. Potential$\backslash$nwater vapor (POWV) or maximum precipitable water vapor can be an$\backslash$nappropriate base for estimation of probable maximum precipitation (PMP)$\backslash$nin an area, leading to probable maximum flood (PMF) and flash flood$\backslash$nmanagement systems. PWV and POWV have miscellaneously been estimated by$\backslash$nmeans of either discrete solutions such as tables, diagrams or empirical$\backslash$nmethods; however, there is no analytical formula for POWV even in a$\backslash$nparticular atmospherical condition. In this article, fundamental$\backslash$ngoverning equations required for analytical calculation of POWV are$\backslash$nfirst introduced. Then, it will be shown that this POWV calculation$\backslash$nrelies on a Riemann integral solution over a range of altitude whose$\backslash$nintegrand is merely a function of altitude. The solution of the integral$\backslash$ngives rise to a series function which is bypassed by approximation of$\backslash$nsaturation vapor pressure in the range of -55 to 55 degrees Celsius, and$\backslash$nan analytical formula for POWV in an atmosphere of constant lapse rate$\backslash$nis proposed. In order to evaluate the accuracy of the suggested$\backslash$nequation, exact calculations of saturated adiabatic lapse rate (SALR) at$\backslash$ndifferent surface temperatures were performed. The formula was compared$\backslash$nwith both the diagrams from the US Weather Bureau and SALR. The results$\backslash$ndemonstrated unquestionable capability of analytical solutions and also$\backslash$nequivalent functions.},
author = {Varmaghani, Ali},
doi = {10.3319/TAO.2011.06.28.01(A)},
file = {:C$\backslash$:/Users/irfanlab/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Varmaghani - 2012 - An Analytical Formula for Potential Water Vapor in an Atmosphere of Constant Lapse Rate.pdf:pdf},
issn = {10170839},
journal = {Terrestrial, Atmospheric and Oceanic Sciences},
keywords = {Lapse rate,Meteorology,Precipitable water vapor,Troposphere},
number = {1},
pages = {17--24},
title = {{An Analytical Formula for Potential Water Vapor in an Atmosphere of Constant Lapse Rate}},
volume = {23},
year = {2012}
}
