isbn = {0003559111111},
issn = {0002-2667},
journal = {Aircraft Engineering and Aerospace Technology},
keywords = {airships,aurora,autonomous unmanned remote monitoring,fluid dynamics,paper type research paper,robotic airship,turbulent wind flow fluctuations,wind-tunnel experiment},
